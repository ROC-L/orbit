CXXFLAGS= -fno-omit-frame-pointer -mno-omit-leaf-frame-pointer -fsized-deallocation -D_FORTIFY_SOURCE=2 -fstack-protector-all
LDFLAGS= -Wl,-z,relro,-z,now,-z,noexecstack
